    "while True:\n",
    "    try:\n",
    "        n = int(input(\"Enter a number: \"))\n",
    "        if n > 1000:\n",
    "            raise ValueError\n",
    "        break\n",
    "    except:\n",
    "        print(\"Invalid\")\n",
    "        continue\n",
    "        \n",
    "sum = 0\n",
    "for i in range(1, n):\n",
    "    sum += n\n",
    "print(sum)"
